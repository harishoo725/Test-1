pipeline {

    agent any

    stages {

        stage('Build') {

            steps {

           echo "build"    

                sh "pythonc Helloworld.py"

            }

        }

        stage('execute') {

            steps {

               echo "test"

                sh "python Helloworld"

            }

        }

        stage('Deploy') {

            steps {

              echo "deploy"

            }

        }

    }

}